<?xml version="1.0" encoding="utf-8"?>
 <odoo>
  <data>
    <record id="configuracion_tipo_equipos_action" model="ir.actions.act_window">
        <field name="name">Sitecnet Configuracion Tipo Equipos</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sitecnet.tipo_equipo</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            Tipo Equipo
          </p>
          <p>
            Presiona Crear para agregar un nuevo tipo de equipos
          </p>
        </field>
      </record>


    <record id="view_sitecnet_equipo_tipo_tree" model="ir.ui.view">
      <field name="name">view.sitecnet.equipo.tipo.tree</field>
      <field name="model">sitecnet.tipo_equipo</field>
      <field name="arch" type="xml">
        <tree string="Equipos Tipo Tree">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="configuracion_subtipo_equipos_action" model="ir.actions.act_window">
        <field name="name">Sitecnet Configuracion Subtipo Equipos</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sitecnet.subtipo_equipo</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            Subtipo Equipo
          </p>
          <p>
            Presiona Crear para agregar un nuevo subtipo de equipos
          </p>
        </field>
      </record>

    <record id="view_sitecnet_equipo_subtipo_tree" model="ir.ui.view">
      <field name="name">view.sitecnet.equipo.subtipo.tree</field>
      <field name="model">sitecnet.subtipo_equipo</field>
      <field name="arch" type="xml">
        <tree string="Equipos Subtipo Tree">
          <field name="name"/>
            <field name="tipo"/>
        </tree>
      </field>
    </record>

