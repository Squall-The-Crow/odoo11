<?xml version="1.0"?>
<openerp>
    <data>
        
        <record id="view_tickets_form_inherit_sitecnet_module" model="ir.ui.view">
            <field name="name">tickets.sitecnet</field>
            <field name="model">website.support.ticket</field>
            <field name="inherit_id" ref="website_support.website_support_ticket_view_form"/>
            <field name="priority" eval="60"/>
            <field name="arch" type="xml">
                <field name="state" position="after">
                    <field name="ubicacion"/>
                </field>
                <field name="partner_id" position="before">
                    <field name="empresa" options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                </field>
                <field name="partner_id" position="replace">
                    <field name="partner_id" domain="[('parent_id','=', empresa)]" options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                </field>
                <field name="approval_id" position="after">
                    <field name="elevado"/>
                </field>                  
                <field name="state" position="after">
                    <field name="usuario" domain="[('parent_id','=', empresa)]" options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                </field>
                <field name="usuario" position="after">
                    <field name="equipo" domain="[('usuario','=', usuario)]" options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                </field>
                <field name="equipo" position="after">
                    <field name="conexion"/>
                </field>
                <field name="conexion" position="after">
                    <field name="telefono"/>
                </field>
                <field name="telefono" position="after">
                    <field name="cel"/>
                </field>
                <notebook position="inside">
                    <page string="Actividades">
                            <group>
                                <field name="actividades"/>
                            </group>
                    </page>                    
                </notebook>
            </field>
       </record>
       
   </data>
</openerp>