<?xml version="1.0"?>
<openerp>
    <data>
        
        <record id="view_documents_form_inherit_sitecnet_module" model="ir.ui.view">
            <field name="name">view.documents.inherit.form</field>
            <field name="model">muk_dms.file</field>
            <field name="inherit_id" ref="muk_dms.view_dms_file_form"/>
            <field name="arch" type="xml">
                <field name="category" position="after">
                    <field name="empresa"/>
                </field>
                <field name="empresa" position="after">
                    <field name="usuario"
                    domain="[('parent_id','=', empresa)]"
                    options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                </field>                
                <notebook position="inside">
                    <page string="Software">
                        <group>
                            <field name="software"
                            domain="[('empresa','=', empresa)]"
                            />
                        </group>
                    </page>
                    <page string="Equipos">
                        <group>
                            <field name="fequipos_cliente"
                            domain="[('empresa','=', empresa)]"
                            options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                            <field name="fequipos_interna"
                            domain="[('empresa','=', empresa)]"
                            options="{'no_create_edit': True},
                           {'no_quick_create'}"/>
                        </group>
                    </page>
                </notebook>
            </field>
       </record>
       
   </data>
</openerp>